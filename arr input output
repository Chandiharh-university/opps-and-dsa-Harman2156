#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the number of elements you want to input: " << endl;
    cin >> n;

    // Input validation
    if (n <= 0) {
        cout << "Invalid number of elements!" << endl;
        return 1;
    }

    int arr[n];

    cout << "Enter elements: " << endl;
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << "Entered elements are: " << endl;
    for (int i = 0; i < n; i++) {
        cout << arr[i] << endl;
    }

    return 0;
}
